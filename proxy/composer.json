{
    "name": "win2k-ai/proxy",
    "description": "LLM Proxy for Windows 2000 AI Client",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.5",
        "symfony/framework-bundle": "^8.0",
        "symfony/http-client": "^8.0",
        "symfony/messenger": "^8.0",
        "symfony/yaml": "^8.0",
        "symfony/dotenv": "^8.0",
        "symfony/runtime": "^8.0"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.60"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "8.0.*"
        }
    }
}
